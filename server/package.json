{
  "name": "teahouse",
  "version": "1.0.0",
  "description": "teahouse eggjs server",
  "private": true,
  "egg": {
    "declarations": true
  },
  "dependencies": {
    "crypto": "^1.0.1",
    "egg": "^2.15.1",
    "egg-jwt": "^3.1.6",
    "egg-mongoose": "^3.2.0",
    "egg-scripts": "^2.11.0",
    "qiniu": "^7.2.2"
  },
  "devDependencies": {
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "egg-bin": "^4.11.0",
    "egg-ci": "^1.11.0",
    "egg-mock": "^3.21.0",
    "eslint": "^5.13.0",
    "eslint-config-egg": "^7.1.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "start": "egg-scripts start --daemon --title=egg-server-teahouse",
    "stop": "egg-scripts stop --title=egg-server-teahouse",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test": "npm run lint -- --fix && npm run test-local",
    "test-local": "egg-bin test",
    "cov": "egg-bin cov",
    "lint": "eslint .",
    "ci": "npm run lint && npm run cov",
    "autod": "autod"
  },
  "ci": {
    "version": "10"
  },
  "repository": {
    "type": "git",
    "url": "http://git.luckincoffee.com/tech-train/ttp5-luckyshop-xianlin.dong"
  },
  "author": "xianlin.dong",
